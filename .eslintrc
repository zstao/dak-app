{
    "env": {
        "browser": true,
        "node": true,
        "amd": true
    },
    "globals": {
        "axios": true,
        "LANG": true,
        "Ecpkn": true
    },
    "rules": {
        "quotes": [1, "single"],
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "no-use-before-define": 0,
        "no-extra-semi": 0,
        "no-wrap-func": 0,
        "no-alert": 0,
        "new-cap": 0,
        "handle-callback-err": 1,
        "strict": 1,
        "curly": 0,
        "no-new": 0
    }
}
